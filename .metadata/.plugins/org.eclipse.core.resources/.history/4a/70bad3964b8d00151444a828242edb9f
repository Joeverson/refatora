package br.com.nun.refactorCode;

import java.util.ArrayList;
import java.util.Collection;

public class Aluguel {	
	private Fita fitas;
	private Integer diasAlugados;
	private Classificacao classificacao;
		
	public Aluguel(Fita fita, int diasAlugada, int classificacao) {
		this.fitas = fita;		
		this.diasAlugados = diasAlugada;
		this.classificacao=classificacao;
	}

	public Integer getDiasAlugada() {
		return this.diasAlugados;
	}
	
	public Fita getfitas() {
		return this.fitas;
	}	
	
	public Classificacao getClassificacao(){
		return this.classificacao;
	}
}
