package br.com.nun.refactorCode;

abstract class Classificacao {
	public static final int NORMAL = 0;
	public static final int LANCAMENTO = 1;
	public static final int INFANTIL = 2;
	
	public int getClassificacao(String type){
		switch(type){
			case "NORMAL":
				return this.NORMAL;
			case "LANCAMENTO":
				return this.LANCAMENTO;
			case "INFANTIL":
				return this.INFANTIL;				
		}
		
		return 0;
	}
}
